<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:vm="using:JazzNotes.ViewModels"
        xmlns:views="using:JazzNotes.Views"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        x:Class="JazzNotes.Views.MainWindow"
        Icon="/Assets/jazznotes-icon.ico" WindowStartupLocation="CenterScreen"
        Title="JazzNotes" Opened="OnMainWindowOpened" Width="800"
        Content="{Binding Content}" Closed="OnMainWindowClosed" />
